{
  "name": "itms-cms",
  "description": "Payload project created from ts-blank template",
  "version": "0.5.2",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "bin/build-payload",
    "build:server": "tsc",
    "build": "npm run copyfiles && npm run build:payload && npm run build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,ejs}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema"
  },
  "resolutions": {
    "graphql": "15.8.0",
    "**/graphql": "15.8.0"
  },
  "dependencies": {
    "app-package-parser": "https://github.com/zgz682000/app-package-parser.git",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.8",
    "express": "^4.17.1",
    "payload": "^1.5.9",
    "qrcode": "^1.5.1",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^18.14.5",
    "@types/react": "^18.0.28",
    "@types/ws": "^8.5.4",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.8.4"
  }
}
